{
  "name": "before-storybook",
  "version": "3.87.3",
  "description": "RDC Shared UI Component Library",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "build": "build",
    "src": "src"
  },
  "files": [
    "lib",
    "build"
  ],
  "scripts": {
    "build": "NODE_ENV=production sh -c 'yarn build:clean && yarn sd:build && yarn build:cjs && yarn build:esm'",
    "build:clean": "rimraf lib dist",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.build.js ./src -s -d ./lib --ignore '__tests__,**/__mock__,src/**/*.spec.js,src/**/*.e2e.js,test.js,__snapshots__,src/**/*.stories.js,README.md,src/haven-style-dictionary/tokens,src/haven-style-dictionary/formats,src/haven-style-dictionary/tests' ",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.build.js ./src -s -d ./lib/esm --ignore '__tests__,**/__mock__,src/**/*.spec.js,src/**/*.e2e.js,test.js,__snapshots__,src/**/*.stories.js,README.md,src/haven-style-dictionary/tokens,src/haven-style-dictionary/formats,src/haven-style-dictionary/tests' ",
    "create:component": "node ./scripts/createComponent.js",
    "doctoc": "doctoc --update-only --title='## Table of Contents' --maxlevel 2 .",
    "format": "prettier --write './src/**/*.{js,jsx}'",
    "lint": "eslint ./src --fix",
    "uipr": "uipr",
    "start": "storybook dev -p 9009 --disable-telemetry",
    "start:story": "sh scripts/storybook-dev.sh",
    "storybook": "yarn start",
    "storybook:build": "yarn run doctoc && storybook build",
    "storybook:deploy": "rimraf docs && storybook build -c .storybook -o docs",
    "storybook:deploy:docker": "NODE_OPTIONS='--max-old-space-size=2048' storybook build -c .storybook -o docs ",
    "storybook:docker": "storybook dev -p 9009 --ci --quiet",
    "test": "jest --coverage  --passWithNoTests --env=jsdom",
    "test-flaky": "jest --testTimeout=1000",
    "test-quick": "jest --env=jsdom --watch",
    "test-bail": "jest --env=jsdom --bail",
    "test-snaps": "yarn test -u",
    "coverage": "open coverage/lcov-report/index.html",
    "size:why": "yarn build && size-limit --why lib/index.js",
    "changelog": "node scripts/updateChangelog",
    "generate:changelogs": "node ./scripts/generateChangelogSummary.js",
    "generate:icons": "sh scripts/icons/optimizeInputSvgs.sh && svgr --template scripts/icons/template.js -d src/illustrations/icons svgs/icons --index-template scripts/svgrIndexTemplate.js",
    "generate:pictos": "sh scripts/icons/optimizeInputSvgs.sh && svgr --template scripts/pictos/template.js -d src/illustrations/pictos svgs/pictos --index-template scripts/svgrIndexTemplate.js",
    "generate:metadata": "node ./scripts/generateMetadataSummary.js",
    "generate:metadata:storybook": "node ./scripts/generateLinksStorybook.js",
    "generate:metadata:github": "node ./scripts/generateLinksGithub.js",
    "generate:metadata:history": "node ./scripts/generateChangelogHistory.js",
    "sd:build": "yarn sd:clean && node ./scripts/haven-style-dictionary/build.js && node ./scripts/haven-style-dictionary/generateIndexFiles.js && cp -r build src/haven-style-dictionary && node ./scripts/haven-style-dictionary/generateDocs.js",
    "sd:clean": "rimraf build && rimraf src/haven-style-dictionary/build && rimraf src/haven-style-dictionary/docs",
    "prepare": "husky install",
    "test:playwright": "playwright test",
    "test:playwright:headed": "HEADLESS='false' playwright test",
    "scan:circular-deps": "node ./scripts/scanCircularDeps.js"
  },
  "dependencies": {
    "css-mediaquery": "^0.1.2",
    "date-fns": "^2.25.0",
    "deepmerge": "3.2.1",
    "downshift": "5.2.1",
    "focus-trap-react": "^8.7.0",
    "isomorphic-dompurify": "^0.14.0",
    "prop-types": "15.7.2",
    "react-is": "16.13.1",
    "react-lazyload": "2.6.2",
    "react-modal": "^3.13.1",
    "react-popper": "2.2.5",
    "react-range": "^1.8.11",
    "react-slick": "0.28.1",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.5.0",
    "@axe-core/react": "^4.5.0",
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.19.1",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-transform-object-assign": "7.8.3",
    "@babel/plugin-transform-react-constant-elements": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/register": "7.4.4",
    "@etchteam/storybook-addon-status": "^4.2.4",
    "@optimizely/optimizely-sdk": "^4.9.1",
    "@playwright/test": "^1.27.0",
    "@size-limit/preset-big-lib": "^8.1.2",
    "@storybook/addon-a11y": "^7.4.5",
    "@storybook/addon-console": "^2.0.0",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-mdx-gfm": "^7.4.5",
    "@storybook/addon-storysource": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-webpack5": "^7.4.5",
    "@svgr/cli": "^6.5.1",
    "@testing-library/dom": "^7.22.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "14.2.5",
    "@welldone-software/why-did-you-render": "^6.2.3",
    "babel-jest": "29.6.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-dev-warning": "0.1.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "cross-env": "^7.0.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "^6",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-perf": "^3.3.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "get-contrast": "^3.0.0",
    "glob-loader": "^0.3.0",
    "highlight.js": "^11.9.0",
    "husky": "^7.0.2",
    "jest": "29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-junit": "^16.0.0",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.2.0",
    "madge": "^5.0.1",
    "playwright": "^1.27.0",
    "prettier": "^1.18.2",
    "react": "^17.x.x",
    "react-dom": "^17.x.x",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "size-limit": "^8.1.2",
    "storybook": "^7.4.6",
    "storybook-addon-performance": "^0.17.1",
    "storybook-addon-turbo-build": "^2.0.1",
    "style-dictionary": "^3.8.0",
    "styled-components": "^5.3.5",
    "svgo": "^3.0.2"
  },
  "resolutions": {
    "escodegen": "2.1.0",
    "semver": "7.5.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json}": [
      "prettier --write"
    ],
    "*.mdx": [
      "yarn run doctoc"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MoveRDC/rdc-ui"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "rdc engineering",
  "license": "ISC",
  "packageManager": "yarn@4.0.2"
}
